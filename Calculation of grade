#include<stdio.h>
#include<conio.h>
void main()
{
int marks;
printf("Please enter your marks");
scanf(%d,&marks);
if(marks>=85);
{
printf("Grade A");
}
else if(marks>=70 && marks<85)
{
printf("Grade B");
}
else if(marks>=55 && marks<70)
{
printf("Grade C");
}
else if(marks>=45 && marks<55)
{
printf("Grade D");
}
else
{
printf("Failed"):
}
}
